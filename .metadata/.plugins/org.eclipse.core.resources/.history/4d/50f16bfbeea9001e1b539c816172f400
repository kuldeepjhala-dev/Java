	public static void main(String[] args) {
		
		try
		{
			DatagramSocket sender = new DatagramSocket(4321);
			byte b[] = new byte[1024];
			String msg = "";
			DatagramPacket pack = null;
		}
		catch(Exception e)
		{
			System.out.println("MySender : " + e.getMessage());
		}

	}

}
